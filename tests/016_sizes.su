lua.sizes = <<1:4, 15:4, 255:8, 1000:16, 100000:32>>

match lua.sizes {
    <<a:4, b:4, c:8, d:16, e:32>> -> {
        lua.print("4-bit values:", a, b)
        lua.print("8-bit value:", c)
        lua.print("16-bit value:", d)
        lua.print("32-bit value:", e)
    },
    _ -> lua.print("Match failed")
}